{
  "name": "blackfish",
  "version": "0.0.0-development",
  "description": "docker* cli extensions",
  "main": "src/index.js",
  "bin": {
    "blackfish": "src/bin.js",
    "bf": "src/bin.js"
  },
  "scripts": {
    "test": "nyc --reporter=lcov ava test/**/*.test.js",
    "precommit": "run-p check lint test check-vulnerablities",
