  "bugs": {
    "url": "https://github.com/cdaringe/blackfish/issues"
  },
  "homepage": "https://github.com/cdaringe/blackfish#readme",
  "devDependencies": {
    "ava": "^2.1.0",
    "counsel": "^0.5.2",
    "counsel-ruleset-nodelib": "^0.5.2",
    "husky": "^2.0.0",
    "jsdock": "^1.0.4",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "nyc": "^14.0.0",
    "semantic-release": "^15.0.0",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
